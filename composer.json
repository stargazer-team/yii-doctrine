{
    "name": "stargazer-team/yii-doctrine",
    "description": "Yii3 integration Doctrine extension",
    "type": "library",
    "keywords": ["yii", "doctrine", "orm", "migrations"],
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/stargazer-team/yii-doctrine"
    },
    "authors": [
        {
            "name": "Stargazer Team",
            "email": "fcnybok@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "doctrine/orm": "^3.1.3",
        "doctrine/migrations": "^3.7.4",
        "psr/cache": "^2.0|^3.0",
        "symfony/cache": "^6.4.7",
        "psr/log": "^2|^3",
        "yiisoft/aliases": "^3.0",
        "yiisoft/config": "^1.5",
        "yiisoft/injector": "^1.2",
        "yiisoft/yii-console": "^2.2.0",
        "yiisoft/definitions": "^3.3"
    },
    "require-dev": {
        "roave/infection-static-analysis-plugin": "^1.29",
        "vimeo/psalm": "^4.30|^5.9",
        "rector/rector": "^0.15.25"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Yii\\Doctrine\\": "src"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true,
            "composer/package-versions-deprecated": true,
            "yiisoft/config": true
        }
    },
    "extra": {
        "config-plugin-options": {
            "source-directory": "config"
        },
        "config-plugin": {
            "params": "params.php",
            "di": "di.php",
            "di-console": "di-console.php"
        }
    }
}
